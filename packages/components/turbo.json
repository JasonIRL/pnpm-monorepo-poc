{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "!loader", "!www", "!**/*.stories.{tsx,jsx,mdx,ts}"]
    },
    "test": {
      "dependsOn": ["^test"],
      "inputs": ["$TURBO_DEFAULT$", "!loader", "!www", "!**/*.stories.{tsx,jsx,mdx,ts}"]
    }
  }
}
